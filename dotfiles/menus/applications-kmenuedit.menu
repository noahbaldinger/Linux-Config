   <Filename>firewall-config.desktop</Filename>
   <Filename>nvidia-settings.desktop</Filename>
   <Filename>pavucontrol.desktop</Filename>
   <Filename>systemsettings.desktop</Filename>
  </Layout>
  <Exclude>
   <Filename>arandr.desktop</Filename>
   <Filename>thunar-settings.desktop</Filename>
   <Filename>yad-settings.desktop</Filename>
  </Exclude>
 </Menu>
 <Menu>
  <Name>System</Name>
  <Layout>
   <Merge type="menus"/>
   <Menuname>Terminal</Menuname>
